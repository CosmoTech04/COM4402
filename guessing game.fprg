<flowgorithm fileversion="4.2">
    <attributes>
        <attribute name="name" value="guessing game"/>
        <attribute name="authors" value="cp5crt"/>
        <attribute name="about" value=""/>
        <attribute name="saved" value="2025-12-11 10:21:13 AM"/>
